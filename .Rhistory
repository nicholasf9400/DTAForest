for (i in 1:nrow(d)) {
temp <- epi.tests(c(d$TP[i], d$FP[i], d$FN[i], d$TN[i]))
temp <- temp[[1]][c(3,4),]
sesp_temp[i,] <- as.numeric(cbind(temp[1,2:4], temp[2,2:4]))
}
colnames(sesp_temp) <- c('sens', 'sensLL', 'sensUL',
'spec', 'specLL', 'specUL')
d <- cbind(d, sesp_temp)
